// Klasa Database

using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

using System.Net.Mail;
using System.Configuration;

using System.Data.SqlClient;
using System.Data;
using System.Net;
namespace LexoSkedarin
{
    public class Skedari
    {
        
        
        public string Adressa { get; set; }
        
        public string Subjekti { get; set; }
        public string Trupi { get; set; }



       
    }


    public class Db
    {
        private string _subjekt;
        private string _adres;
        private string _trup;
        private SqlConnection _sql = new SqlConnection();
        public Db(SqlConnection sqlcon)
        {

            _sql = sqlcon;
        }
        public string Subjekti
        {
            get {
                return _subjekt; }
            set { _subjekt = value; }
        }
        public string Adresa
        {
            get
            {
                return _adres;
           }
            set { _adres = value; }


        }
        public string Trupi
        {
            get { return _trup; }
            set { _trup = value; }
        }

        public bool RuajTedhenat()
        {
            bool check = false;
            SqlCommand ccm = new SqlCommand();
            ccm.CommandType = CommandType.StoredProcedure;
            ccm.CommandText = "pro_addSkedare";
            ccm.Parameters.Add("@subjek", SqlDbType.VarChar).Value=Subjekti;
            ccm.Parameters.Add("@trupi", SqlDbType.VarChar).Value=Trupi;
            ccm.Parameters.Add("@adresa", SqlDbType.VarChar).Value=Adresa;
            if(ccm.ExecuteNonQuery()>0)
            {
                check=true;

            }
            ccm.Dispose();
            return check;


        }

    }
}
   
   //lexo skedarin
   using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

using System.Net.Mail;
using System.Configuration;

using System.Data.SqlClient;
using System.Data;
using System.Net;
namespace LexoSkedarin
{
    public class Skedari
    {
        
        
        public string Adressa { get; set; }
        
        public string Subjekti { get; set; }
        public string Trupi { get; set; }



       
    }


    public class Db
    {
        private string _subjekt;
        private string _adres;
        private string _trup;
        private SqlConnection _sql = new SqlConnection();
        public Db(SqlConnection sqlcon)
        {

            _sql = sqlcon;
        }
        public string Subjekti
        {
            get {
                return _subjekt; }
            set { _subjekt = value; }
        }
        public string Adresa
        {
            get
            {
                return _adres;
           }
            set { _adres = value; }


        }
        public string Trupi
        {
            get { return _trup; }
            set { _trup = value; }
        }

        public bool RuajTedhenat()
        {
            bool check = false;
            SqlCommand ccm = new SqlCommand();
            ccm.CommandType = CommandType.StoredProcedure;
            ccm.CommandText = "pro_addSkedare";
            ccm.Parameters.Add("@subjek", SqlDbType.VarChar).Value=Subjekti;
            ccm.Parameters.Add("@trupi", SqlDbType.VarChar).Value=Trupi;
            ccm.Parameters.Add("@adresa", SqlDbType.VarChar).Value=Adresa;
            if(ccm.ExecuteNonQuery()>0)
            {
                check=true;

            }
            ccm.Dispose();
            return check;


        }

    }
}
   
 

  // UnitTest Skedari
  
  using System;
using Microsoft.VisualStudio.TestTools.UnitTesting;

namespace UnitTestProject1
{
    [TestClass]
    public class UnitTest1
    {
        private TestContext test;
        public TestContext Test
        {

            get
            {
                return test;
            }
            set { test = value; }

        }
    [TestMethod]
        public void Subjekti()
        {
            string subjekt ="hell";
            string trupi = "Detyra";
            bool actual = LexoSkedarin.Skedari.ReferenceEquals(subjekt, trupi);


            Assert.AreEqual(subjekt, trupi);



        }
        
   
    }
}


// NunitTest 

using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using NUnit.Framework;

namespace SkedarNunit
{ [TestFixture]
    public class Class1
    { [Test]
        public void Subjekti()
        {
            string subjekt = "hello";
            string trupi = "Detyra";

            Assert.AreEqual(subjekt, trupi);



        }
    }
}


