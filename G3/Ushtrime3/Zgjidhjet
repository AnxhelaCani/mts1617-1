From1()

using System;
using System.Collections.Generic;
using System.ComponentModel;
using System.Data;
using System.Drawing;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Windows.Forms;
using System.IO;
using System.Data;
using System.Net;
using System.Net.Mail;
using CsvHelper;

namespace LexoSkedarin
{
    public partial class Form1 : Form
    {
        public Form1()
        {
            InitializeComponent();
        }

       

        private void btWrite_Click(object sender, EventArgs e)
        {
            using (SaveFileDialog sfd = new SaveFileDialog()
                {
                Filter = "CSV|*.csv", ValidateNames =true })
            {

                if ( sfd.ShowDialog() == DialogResult.OK)
                {
                    using (var sw=new  StreamWriter(sfd.FileName))
                    {
                        var dergo = new CsvWriter(sw);
                        dergo.WriteHeader(typeof(Skedari));
                        foreach(Skedari s in skedariBindingSource.DataSource as List<Skedari>)
                        {
                            dergo.WriteRecord(s);

                        }

                    }

                    MessageBox.Show("Te dhenat e tuja u ruajten me sukses", "Mesazhi", MessageBoxButtons.OK, MessageBoxIcon.Information);

                }
            }
        }
    private void Form1_Load(object sender, EventArgs e)
        {
            skedariBindingSource.DataSource = new List<Skedari>();
         

        }

    private void btRead_Click(object sender, EventArgs e)
    {
        using (OpenFileDialog ofd=new OpenFileDialog () 
            {
             Filter = "CSV|*.csv", ValidateNames =true })
        {
            if(ofd.ShowDialog()==DialogResult.OK)
            {
                var sr = new StreamReader(new FileStream(ofd.FileName, FileMode.Open));
                var csv = new CsvReader(sr);
                skedariBindingSource.DataSource = csv.GetRecords<Skedari>();

            }
        }
           
    }

    private void button1_Click(object sender, EventArgs e)
    {
        var result = new email();
        result.Dergo();

    }
    
    }


}

